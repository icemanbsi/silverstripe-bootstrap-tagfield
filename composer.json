    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "bootstrap", "tags", "twitter"],
	  "license": "BSD-3-Clause",
    "authors": [
    {
        "name": "Uncle Cheese",
        "email": "unclecheese@leftandmain.com"
    }
    ],
    "require":
    {
      "silverstripe/framework": "^4.1",
      "silverstripe/cms": "^4.1"
    },
    "autoload": {
      "psr-4": {
        "UncleCheese\\BootstrapTagField\\": "code/"
      }
    },
    "extra": {
      "installer-name": "bootstrap-tagfield",
      "expose": [
          "javascript",
          "css"
      ]
    }   
